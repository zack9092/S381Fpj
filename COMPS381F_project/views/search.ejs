<html>
    <head>
        <title>Search Restaurant</title>
    </head>
    <body>
        <form action="/search" method="GET">
            restaurant_id:<br>
            <input type="text" name="restaurant_id">
            <br>
            name:<br>
            <input type="text" name="name">
            <br>
            borough:<br>
            <input type="text" name="borough">
            <br>
            cuisine:<br>
            <input type="text" name="cuisine">
            <br>
            street:<br>
            <input type="text" name="address.street">
            <br>
            building:<br>
            <input type="text" name="address.building">
            <br>
            zipcode:<br>
            <input type="text" name="address.zipcode">
            <br>
            X-coord:<br>
            <input type="text" name="address.coord.0">
            <br>
            Y-coord:<br>
            <input type="text" name="address.coord.1">
            <br>
            owner:<br>
            <input type="text" name="owner">
            <br>
            <input type="submit" value="Submit">
          </form> 
    </body>
</html>